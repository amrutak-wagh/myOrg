        <fullName>Grace_Period_must_fit_within_Quote_dates</fullName>
        <active>true</active>
        <description>Grace period start date and duration must fit within duration of the lease</description>
        <errorConditionFormula>(Start_Date__c &lt; Quote__r.Proposed_Start_Date__c) || (Start_Date__c &gt; Quote__r.Proposed_End_Date__c) || ((Start_Date__c + Duration__c) &gt; Quote__r.Proposed_End_Date__c)</errorConditionFormula>
        <errorMessage>Grace period must fall within the duration of the lease.</errorMessage>
    </validationRules>
