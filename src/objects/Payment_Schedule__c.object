    <validationRules>
        <fullName>Cannot_update_when_qoute_under_approval</fullName>
        <active>false</active>
        <description>Cannot update record if quote status is under approval.</description>
        <errorConditionFormula>AND( $Profile.Name !=&apos;System Administrator&apos;, OR(ISPICKVAL(Quote__r.Status ,&quot;Approved&quot;),ISPICKVAL(Quote__r.Status ,&quot;Under Approval&quot;),
ISPICKVAL(Quote__r.Status ,&quot;Synched&quot;),
ISPICKVAL(Quote__r.Status ,&quot;Accepted&quot;)))</errorConditionFormula>
        <errorMessage>Cannot update the payment schedules when quote is under DOA approval.</errorMessage>
    </validationRules>
