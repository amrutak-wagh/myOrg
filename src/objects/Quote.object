    <validationRules>
        <fullName>Description_field_cannot_exceed_2000</fullName>
        <active>true</active>
        <errorConditionFormula>$Profile.Name &lt;&gt; &quot;Integration User Profile&quot; &amp;&amp;
$Profile.Name &lt;&gt; &quot;System Administrator&quot; &amp;&amp;
LEN(Description )  &gt;=  2000</errorConditionFormula>
        <errorDisplayField>Description</errorDisplayField>
        <errorMessage>The description cannot be longer than 2000 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Do_not_allow_creation_of_new_Opportunity</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp;  NOT(ISBLANK(Opportunity.SyncedQuoteId))&amp;&amp;
$Profile.Name &lt;&gt; &quot;System Administrator&quot;</errorConditionFormula>
        <errorMessage>Opportunity selected by you already have synced quote. Please choose another</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_not_before_Start_Date</fullName>
        <active>true</active>
        <description>Validate that the Lease End Date can not be before the Lease Start Date or TODAY.</description>
        <errorConditionFormula>$Profile.Name &lt;&gt; &quot;Integration User Profile&quot; &amp;&amp; 
OR( Proposed_End_Date__c &lt; Proposed_Start_Date__c , 
Proposed_End_Date__c = Proposed_Start_Date__c )</errorConditionFormula>
        <errorMessage>The Proposed End Date can not be the same or before the Proposed Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_quote_when_approved_forSystem_Admin</fullName>
        <active>false</active>
        <description>Throws error if user tries to edit record after approval.</description>
        <errorConditionFormula>AND( ISPICKVAL(PRIORVALUE(Status),&quot;Approved&quot;),
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name == &apos;System Administrator&apos;,  
Soft_Book__c == false,
OR(   
ISPICKVAL(Status, &quot;Approved&quot;),
ISPICKVAL(Status, &quot;Accepted&quot;), 
ISPICKVAL(Status, &quot;Synched&quot;), 
ISPICKVAL(Status, &quot;Rejected&quot;), 
ISPICKVAL(Status, &quot;Cancelled&quot;), 
ISPICKVAL(Status, &quot;CCL Acceptance&quot;))
)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>Cannot edit quote once approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opp_Required_Fields_before_quote_create</fullName>
        <active>false</active>
        <errorConditionFormula>$Profile.Name &lt;&gt; &quot;Integration User Profile&quot; &amp;&amp;
OR(Opportunity.Business_Park__c = &quot;&quot;, 
  ISPICKVAL( Opportunity.Lease_Type__c, &quot;&quot;)
)</errorConditionFormula>
        <errorMessage>Please complete Business Park, Opportunity Type and Lease Type before creating a new Quote.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_cannot_be_closed_lost</fullName>
        <active>true</active>
        <description>Quote cannot be created if the related opportunity is closed lost.</description>
        <errorConditionFormula>$Profile.Name &lt;&gt; &quot;Integration User Profile&quot; &amp;&amp; 
$Profile.Name &lt;&gt; &quot;System Administrator&quot; &amp;&amp; ISNEW() &amp;&amp; ISPICKVAL(Opportunity.StageName, &apos;Closed Lost&apos;)</errorConditionFormula>
        <errorMessage>You cannot create a quote against a &apos;Closed Lost&apos; Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quote_Synched</fullName>
        <active>true</active>
        <description>Don&apos;t allow for creation of Quote when another is synched,</description>
        <errorConditionFormula>$Profile.Name &lt;&gt; &apos;System Administrator&apos; &amp;&amp;
NOT(ISPICKVAL(Status , &quot;Cancelled&quot;)) &amp;&amp; Opportunity.SyncedQuoteId != NULL &amp;&amp; Opportunity.SyncedQuoteId!= Id</errorConditionFormula>
        <errorMessage>Creation of a new Quote is not allowed if there is a synched quote.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quote_cannot_be_be_updated_back_to_draft</fullName>
        <active>true</active>
        <description>The quote cannot be be updated back to draft.</description>
        <errorConditionFormula>$Profile.Name &lt;&gt; &quot;Integration User Profile&quot; &amp;&amp;
$Profile.Name &lt;&gt; &quot;System Administrator&quot; &amp;&amp;
ISPICKVAL(Opportunity.StageName, &quot;Closed Lost&quot;) &amp;&amp;
ISPICKVAL( PRIORVALUE(Status) , &quot;Cancelled&quot;) &amp;&amp;
ISPICKVAL(Status, &quot;Draft&quot;)</errorConditionFormula>
        <errorMessage>The quote cannot be be updated back to draft.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quote_must_be_Submitted_for_Approval</fullName>
        <active>false</active>
        <description>Will enforce the user to submit for approval through the button and not through the change of status.</description>
        <errorConditionFormula>$Profile.Name &lt;&gt; &quot;Integration User Profile&quot; 
&amp;&amp; 
$Profile.Name &lt;&gt; &quot;System Administrator&quot; 
&amp;&amp; 
ISCHANGED(Status)
&amp;&amp; 
ISPICKVAL(Status, &quot;Under ApprovaL&quot;)
&amp;&amp;
(Manually_submitted__c = FALSE)</errorConditionFormula>
        <errorMessage>Approval needs to be requested by clicking the &apos;Submit for Approval&apos; button.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Specify_Grace_period</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISPICKVAL( Status , &quot;Cancelled&quot;)) &amp;&amp; $Profile.Name &lt;&gt; &quot;Integration User Profile&quot; &amp;&amp;
OR(
AND(Grace_Period__c &gt; 0,
ISBLANK(TEXT(Grace_Period_Period__c))),

AND(Grace_Period__c &gt; 0,
ISBLANK(TEXT(Grace_Period_Mechanism__c))
)
)</errorConditionFormula>
        <errorMessage>Please specify Grace Period (Period) and Grace Period Mechanism.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Sync_Stop_Sync_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>$Profile.Name &lt;&gt; &quot;System Administrator&quot; &amp;&amp;
ISPICKVAL( Status , &apos;Cancelled&apos;) &amp;&amp;  IsSyncing == True</errorConditionFormula>
        <errorMessage>Cancelled Quote cant be synced</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Accepted</fullName>
        <active>true</active>
        <description>Quote in Accepted Status:
 Can only move to Synched as a manual change.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
ISCHANGED(Status), 
ISPICKVAL(PRIORVALUE(Status),&quot;Accepted&quot;), 
OR( 
ISPICKVAL(Status, &quot;Draft&quot;), 
ISPICKVAL(Status, &quot;Under Approval&quot;), 
ISPICKVAL(Status, &quot;Approved&quot;),  
ISPICKVAL(Status, &quot;Rejected&quot;), 
ISPICKVAL(Status, &quot;Cancelled&quot;),
ISPICKVAL(Status, &quot;CCL Acceptance&quot;)
) 
)</errorConditionFormula>
        <errorMessage>An accepted quote can be moved to Synched only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Accepted_CLC</fullName>
        <active>true</active>
        <description>This validation rule prevents a quote to be accepted if the CLC document was not accepted.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
ISCHANGED(Status), 
ISPICKVAL(Status,&quot;Accepted&quot;), 
ISBLANK(CCL_Sign_Date__c)
)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>Please ensure that the CLC document has been accepted prior to approving a quote.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Approved</fullName>
        <active>true</active>
        <description>Quote in Approved Status:
Can only move to either Draft or Cancelled as a manual change.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
ISCHANGED(Status), 
ISPICKVAL(PRIORVALUE(Status),&quot;Approved&quot;), 
OR(
   ISPICKVAL(Status, &quot;Under Approval&quot;), 
   ISPICKVAL(Status, &quot;Accepted&quot;), 
   ISPICKVAL(Status, &quot;Synched&quot;), 
   ISPICKVAL(Status, &quot;Rejected&quot;),
   ISPICKVAL(Status, &quot;CCL Acceptance&quot;)
) 
)</errorConditionFormula>
        <errorMessage>An approved quote can only be changed to Cancelled or Draft only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Approved_AP</fullName>
        <active>true</active>
        <description>Enforces that the status of the quote can only be changed to approved, once it has been through the approval process, otherwise can not be updated.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
ISCHANGED(Status),
ISPICKVAL(Status, &quot;Approved&quot;),
(Quote_Approved_Via_Approval__c = FALSE)
)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>Approval can only be updated after submission through the approval process.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Cancelled</fullName>
        <active>true</active>
        <description>A cancelled quote can be moved to Draft only.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
ISCHANGED(Status), 
ISPICKVAL(PRIORVALUE(Status),&quot;Cancelled&quot;), 
OR( 
ISPICKVAL(Status, &quot;Under Approval&quot;), 
ISPICKVAL(Status, &quot;Approved&quot;), 
ISPICKVAL(Status, &quot;Accepted&quot;), 
ISPICKVAL(Status, &quot;Synched&quot;), 
ISPICKVAL(Status, &quot;Rejected&quot;) 
) 
)</errorConditionFormula>
        <errorMessage>Quote in Cancelled Status: 
Can only move to Draft as a manual change.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Change_Cancelled</fullName>
        <active>true</active>
        <description>Quote in Cancelled Status
1) All quotes can move from their current status to this status except for Accepted.
2) Only allow change to Status Cancelled when Opp is closed lost</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
ISCHANGED(Status),
!ISPICKVAL(Opportunity.StageName, &quot;Closed Lost&quot;),
ISPICKVAL(Status, &quot;Cancelled&quot;),
ISPICKVAL(PRIORVALUE(Status),&quot;Accepted&quot;)
)</errorConditionFormula>
        <errorMessage>You can only change the Status to Cancelled for a Closed Lost Opportunity for non-accepted Quotes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Draft</fullName>
        <active>true</active>
        <description>Throws error if user tries to change status from draft to any other status except UA</description>
        <errorConditionFormula>AND(
    $Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
    $Profile.Name &lt;&gt; &quot;System Administrator&quot;,
    ISCHANGED(Status),
    ISPICKVAL(PRIORVALUE(Status),&quot;Draft&quot;),
    OR(
       ISPICKVAL(Status, &quot;Approved&quot;),
       ISPICKVAL(Status, &quot;Accepted&quot;),
       ISPICKVAL(Status, &quot;Synched&quot;),
       ISPICKVAL(Status, &quot;Rejected&quot;),
       ISPICKVAL(Status, &quot;CCL Acceptance&quot;)
   ) 
)</errorConditionFormula>
        <errorMessage>Drafted quote status can be changed to Under Approval status only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Rejected</fullName>
        <active>true</active>
        <description>Quote in Rejected Status:
Can only move to either Draft or Cancelled as a manual change.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
ISCHANGED(Status), 
ISPICKVAL(PRIORVALUE(Status),&quot;Rejected&quot;), 
OR( 
ISPICKVAL(Status, &quot;Under Approval&quot;), 
ISPICKVAL(Status, &quot;Approved&quot;), 
ISPICKVAL(Status, &quot;Accepted&quot;), 
ISPICKVAL(Status, &quot;Synched&quot;),  
ISPICKVAL(Status, &quot;CCL Acceptance&quot;) 
) 
)</errorConditionFormula>
        <errorMessage>A rejected quote can only be updated to either Draft or Cancelled only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Rejected_AP</fullName>
        <active>true</active>
        <description>Enforces that the status of the quote can only be changed to rejected, once it has been through the approval process, otherwise can not be updated.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
ISCHANGED(Status),
ISPICKVAL(Status, &quot;Rejected&quot;), 
(Quote_Rejected_Via_Approval__c = FALSE) 
)</errorConditionFormula>
        <errorMessage>Rejection can only be updated after submission through the approval process.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Synched</fullName>
        <active>true</active>
        <description>Quote in Synched Status: 
Cannot move to any of the other statuses</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
ISCHANGED(Status), 
ISPICKVAL(PRIORVALUE(Status),&quot;Synched&quot;)
)</errorConditionFormula>
        <errorMessage>You cannot change the status once it&apos;s Synched</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Under_Approval</fullName>
        <active>true</active>
        <description>Throws an error when Under approval status changes to any other status except Approval or rejected.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
ISCHANGED(Status), 
ISPICKVAL(PRIORVALUE(Status),&quot;Under Approval&quot;), 
 OR( 
    ISPICKVAL(Status, &quot;Draft&quot;),  
    ISPICKVAL(Status, &quot;Accepted&quot;), 
    ISPICKVAL(Status, &quot;Synched&quot;),
    ISPICKVAL(Status, &quot;CCL Acceptance&quot;)
   ) 
)</errorConditionFormula>
        <errorMessage>Status can be only changed to Approved or Rejected from Under Approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Validation_Under_Approval_AP</fullName>
        <active>true</active>
        <description>A quote could only go to under approval once it was submitted for approval.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;Integration User Profile&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
ISCHANGED(Status),
ISPICKVAL(Status, &quot;Under Approval&quot;),
(Manually_submitted__c = FALSE) 
)</errorConditionFormula>
        <errorMessage>Please submit for approval</errorMessage>
    </validationRules>
