        <fullName>DOA_Discount__c</fullName>
        <externalId>false</externalId>
        <formula>If( Total_Best_Rate__c &lt;&gt;0, (DOA_Discount_Amount__c+ Additional_GP_Discount_Amount__c -Total_Negative_GP_Discount_Amount__c- Total_Upselling_Discount_Amount__c)/(Total_Best_Rate__c+ Total_Best_Rate_for_Proposed_GP__c ) , 
IF(AND(Total_Best_Rate__c=0,TotalPrice=0),1,0) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>DOA Discount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>DOA_Discount_percent__c</fullName>
        <description>DOA Discount percentage of the total best rate</description>
        <externalId>false</externalId>
        <formula>(Offer_Best_Discount_Amount__c +  Additional_GP_Discount_Amount__c) /  Total_Best_Rate__c</formula>
